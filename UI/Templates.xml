<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\..\Blizzard_SharedXML\UI.xsd">
    <Button name="EmojifyVisualPickerChatButtonTemplate" hidden="true" mixin="EmojifyVisualPickerChatButtonMixin" virtual="true">
        <Size x="22" y="22"/>
        <Layers>
            <Layer level="ARTWORK">
                <Texture parentKey="Texture" setAllPoints="true" desaturated="true"/>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad method="OnLoad"/>
            <OnClick method="OnClick"/>
            <OnEnter method="OnEnter"/>
            <OnLeave method="OnLeave"/>
        </Scripts>
    </Button>

    <Button name="EmojifyAutocompleteButtonTemplate" mixin="EmojifyAutocompleteButtonMixin" virtual="true">
        <Size x="1" y="28"/>
        <Layers>
            <Layer level="BACKGROUND" textureSubLevel="0">
                <Texture parentKey="Highlight" hidden="true">
                    <Color r="0.3" g="0.3" b="0.3" a="0.3"/>
                </Texture>
            </Layer>
            <Layer level="BACKGROUND" textureSubLevel="1">
                <Texture parentKey="Texture">
                    <Size x="24" y="24"/>
                    <Anchors>
                        <Anchor point="LEFT" x="4"/>
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="BACKGROUND" textureSubLevel="1">
                <FontString parentKey="Text" inherits="GameFontNormal" justifyH="LEFT" justifyV="MIDDLE">
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent.Texture" relativePoint="RIGHT" x="6"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnClick method="OnClick"/>
            <OnEnter method="OnEnter"/>
            <OnLeave method="OnLeave"/>
        </Scripts>
    </Button>
    <Frame name="EmojifyAutocompleteFrame" parent="UIParent" frameStrata="DIALOG" toplevel="true" enableMouse="true" mixin="EmojifyAutocompleteFrameMixin" inherits="BackdropTemplate" hidden="true">
        <Size x="240" y="100"/>
        <Frames>
            <Frame parentKey="ScrollBox" inherits="WowScrollBoxList">
                <Anchors>
                    <Anchor point="TOPLEFT" x="8" y="-4"/>
                    <Anchor point="BOTTOMRIGHT" x="-26" y="4"/>
                </Anchors>
            </Frame>
            <EventFrame parentKey="ScrollBar" inherits="MinimalScrollBar">
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent.ScrollBox" relativePoint="TOPRIGHT" x="7" y="-3"/>
                    <Anchor point="BOTTOMLEFT" relativeKey="$parent.ScrollBox" relativePoint="BOTTOMRIGHT" x="7" y="2"/>
                </Anchors>
            </EventFrame>
            <Frame>
                <Anchors>
                    <Anchor point="TOPLEFT" x="4" y="-4"/>
                    <Anchor point="BOTTOMRIGHT" x="-4" y="4"/>
                </Anchors>
                <Layers>
                    <Layer level="OVERLAY" textureSubLevel="7">
                        <FontString parentKey="HintText" inherits="GameFontNormalSmall" justifyH="CENTER" text="EMOJIFY_AUTOCOMPLETE_HINT">
                            <Anchors>
                                <Anchor point="BOTTOM" x="-10" y="5"/>
                            </Anchors>
                            <Color r="0.6" g="0.6" b="0.6"/>
                        </FontString>
                    </Layer>
                    <Layer level="OVERLAY" textureSubLevel="6">
                        <Texture>
                            <Size y="32"/>
                            <Anchors>
                                <Anchor point="BOTTOMLEFT"/>
                                <Anchor point="BOTTOMRIGHT"/>
                            </Anchors>
                            <Color r="0" g="0" b="0" a="1"/>
                            <Gradient orientation="HORIZONTAL">
                                <MaxColor r="0" g="0" b="0" a="0"/>
                                <MinColor r="0" g="0" b="0" a="1"/>
                            </Gradient>
                        </Texture>
                    </Layer>
                </Layers>
            </Frame>
        </Frames>
        <Scripts>
            <OnLoad method="OnLoad"/>
            <OnShow method="OnShow"/>
            <OnHide method="OnHide"/>
        </Scripts>
    </Frame>

    <Frame name="EmojifyVisualPickerElementTemplate" virtual="true">
        <Size x="1" y="28"/>
    </Frame>
    <Button name="EmojifyVisualPickerPackHeaderButtonTemplate" registerForClicks="LeftButtonUp, RightButtonUp" mixin="EmojifyVisualPickerPackHeaderMixin" virtual="true">
        <Size x="289" y="22"/>
        <KeyValues>
            <KeyValue key="leftPadding" value="9" type="number"/>
        </KeyValues>
        <ButtonText parentKey="ButtonText" nonspacewrap="true" justifyH="LEFT">
            <Size x="234" y="10"/>
            <Anchors>
                <Anchor point="LEFT" x="8" y="1"/>
            </Anchors>
        </ButtonText>
        <NormalTexture atlas="questlog-tab"/>
        <HighlightTexture atlas="questlog-tab" alphaMode="ADD" alpha="0.4"/>
        <NormalFont style="GameFontNormal"/>
        <HighlightFont style="GameFontNormal"/>
        <Frames>
            <Button parentKey="CollapseButton" mixin="EmojifyVisualPickerPackHeaderCollapseMixin" propagateMouseInput="Both">
                <Size x="16" y="16"/>
                <Anchors>
                    <Anchor point="RIGHT" x="-4"/>
                </Anchors>
                <Layers>
                    <Layer level="ARTWORK">
                        <Texture parentKey="Icon" setAllPoints="true"/>
                    </Layer>
                </Layers>
                <HighlightTexture alphaMode="ADD" alpha="0.4">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.Icon"/>
                        <Anchor point="BOTTOMRIGHT" relativeKey="$parent.Icon"/>
                    </Anchors>
                </HighlightTexture>
            </Button>
        </Frames>
        <Scripts>
            <OnLoad method="OnLoad"/>
            <OnClick method="OnClick"/>
            <OnEnter method="OnEnter"/>
            <OnLeave method="OnLeave"/>
            <OnMouseDown method="OnMouseDown"/>
            <OnMouseUp method="OnMouseUp"/>
        </Scripts>
    </Button>
    <Button name="EmojifyVisualPickerGridButtonTemplate" mixin="EmojifyVisualPickerGridMixin" virtual="true">
        <Size x="48" y="48"/>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="Background">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="2" y="-2"/>
                        <Anchor point="BOTTOMRIGHT" x="-2" y="2"/>
                    </Anchors>
                    <Color r="0" g="0" b="0" a="0.5"/>
                </Texture>
            </Layer>
            <Layer level="ARTWORK">
                <Texture parentKey="Texture">
                    <Size x="40" y="40"/>
                    <Anchors>
                        <Anchor point="CENTER"/>
                    </Anchors>
                </Texture>
                <MaskTexture parentKey="IconMask" atlas="item_upgrade_tooltip_fullmask" hWrapMode="CLAMPTOBLACKADDITIVE" vWrapMode="CLAMPTOBLACKADDITIVE">
                    <Size x="40" y="40"/>
                    <Anchors>
                        <Anchor point="CENTER"/>
                    </Anchors>
                    <MaskedTextures>
                        <MaskedTexture childKey="Texture"/>
                    </MaskedTextures>
                </MaskTexture>
            </Layer>
            <Layer level="HIGHLIGHT">
                <Texture parentKey="Highlight" hidden="true">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="2" y="-2"/>
                        <Anchor point="BOTTOMRIGHT" x="-2" y="2"/>
                    </Anchors>
                    <Color r="0.4" g="0.4" b="0.8" a="0.3"/>
                </Texture>
            </Layer>
        </Layers>
        <Scripts>
            <OnClick method="OnClick"/>
            <OnEnter method="OnEnter"/>
            <OnLeave method="OnLeave"/>
        </Scripts>
    </Button>
    <Frame name="EmojifyVisualPickerFrame" parent="UIParent" frameStrata="DIALOG" toplevel="true" enableMouse="true" movable="true" clampedToScreen="true" registerForDrag="LeftButton" inherits="ButtonFrameTemplate" mixin="EmojifyVisualPickerMixin" hidden="true">
        <Size x="380" y="520"/>
        <Anchors>
            <Anchor point="CENTER"/>
        </Anchors>
        <Frames>
            <Frame parentKey="ScrollBox" inherits="WowScrollBoxList">
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent.Inset" relativePoint="BOTTOMLEFT" x="-2" y="3"/>
                    <Anchor point="BOTTOMRIGHT" relativeKey="$parent.Inset" relativePoint="TOPRIGHT" x="2" y="-4"/>
                </Anchors>
            </Frame>
            <EventFrame parentKey="ScrollBar" inherits="MinimalScrollBar">
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent.ScrollBox" relativePoint="TOPRIGHT" x="6" y="2"/>
                    <Anchor point="BOTTOMLEFT" relativeKey="$parent.ScrollBox" relativePoint="BOTTOMRIGHT" x="6" y="-4"/>
                </Anchors>
            </EventFrame>
            <Frame parentKey="BorderFrame" frameLevel="100">
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent.Inset" x="-3" y="3"/>
                    <Anchor point="BOTTOMRIGHT" relativeKey="$parent.Inset" x="3" y="-5"/>
                </Anchors>
                <Layers>
                    <Layer level="BORDER">
                        <Texture parentKey="Border" atlas="questlog-frame" setAllPoints="true"/>
                    </Layer>
                </Layers>
            </Frame>
            <EditBox parentKey="SearchBox" inherits="SearchBoxTemplate" autoFocus="false">
                <Size x="0" y="26"/>
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent.BorderFrame" x="10" y="27"/>
                    <Anchor point="TOPRIGHT" relativeKey="$parent.BorderFrame" x="15" y="27"/>
                </Anchors>
                <Scripts>
                    <OnTextChanged>
                        EmojifyVisualPickerFrame:OnSearchChanged(self, self:GetText());
                    </OnTextChanged>
                </Scripts>
            </EditBox>
            <Frame parentKey="PreviewBar">
                <Size x="0" y="22"/>
                <Anchors>
                    <Anchor point="BOTTOMLEFT" x="14" y="20"/>
                    <Anchor point="BOTTOMRIGHT" x="-10" y="20"/>
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND" textureSubLevel="0">
                        <Texture>
                            <Anchors>
                                <Anchor point="TOPLEFT" x="2" y="-2"/>
                                <Anchor point="BOTTOMRIGHT" x="-2" y="2"/>
                            </Anchors>
                            <Color r="0" g="0" b="0" a="0.5"/>
                        </Texture>
                    </Layer>
                    <Layer level="BACKGROUND" textureSubLevel="1">
                        <Texture setAllPoints="true">
                            <Color r="0.3" g="0.3" b="0.3" a="0.3"/>
                        </Texture>
                    </Layer>
                    <Layer level="ARTWORK">
                        <Texture parentKey="Icon">
                            <Size x="32" y="32"/>
                            <Anchors>
                                <Anchor point="LEFT" x="10" y="0"/>
                            </Anchors>
                        </Texture>
                        <FontString parentKey="Code" inherits="GameFontNormalLarge" justifyH="LEFT">
                            <Anchors>
                                <Anchor point="LEFT" relativeKey="$parent.Icon" relativePoint="RIGHT" x="10"/>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </Frame>
        </Frames>
        <Scripts>
            <OnLoad method="OnLoad"/>
            <OnShow method="OnShow"/>
            <OnHide method="OnHide"/>
            <OnDragStart method="OnDragStart"/>
            <OnDragStop method="OnDragStop"/>
        </Scripts>
    </Frame>
</Ui>